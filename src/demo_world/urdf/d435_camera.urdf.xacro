<?xml version="1.0"?>
<robot name="realsense2_camera" xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:arg name="use_nominal_extrinsics" default="true"/>
  <xacro:include filename="$(find demo_world)/urdf/_d435.urdf.xacro" />
  
  <xacro:sensor_d435 parent="elfin_end_link" use_nominal_extrinsics="$(arg use_nominal_extrinsics)">
    <origin xyz="0.05 0 0" rpy="0 0 0"/>
  </xacro:sensor_d435>
</robot>
